{
  "name": "jupyter-pyllisto",
  "version": "0.1.0",
  "private": true,
  "description": "Render Jupyter widgets in a standalone front-end",
  "license": "BSD-3-Clause",
  "author": "Nicholas Earl",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && tsc && node scripts/copyfiles.js && webpack",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && rimraf built",
    "host": "http-server",
    "lint": "tslint --project tslint.json --format stylish",
    "test": "npm run test:default",
    "test:default": "echo \"No test specified\"",
    "watch": "tsc -w",
    "start": "electron ./lib/main.js"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/controls": "^1.5.2",
    "@jupyter-widgets/html-manager": "^0.18.3",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.2",
    "@jupyterlab/services": "^4.1.0",
    "@phosphor/widgets": "^1.9.1",
    "@types/jquery": "^3.3.29",
    "bqplot": "^0.4.7",
    "bqplot-image-gl": "^0.1.4",
    "codemirror": "^5.47.0",
    "commander": "^2.20.0",
    "font-awesome": "^4.7.0",
    "fs": "^0.0.1-security",
    "http-server": "^0.11.1",
    "jupyter-materialui": "0.0.1-alpha.0",
    "jupyter-matplotlib": "^0.4.2",
    "jupyter-vuetify": "^0.1.11",
    "plotlywidget": "^0.10.0"
  },
  "devDependencies": {
    "@types/codemirror": "^0.0.74",
    "@types/commander": "^2.12.2",
    "css-loader": "^3.0.0",
    "electron": "^5.0.1",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.0.1",
    "json-loader": "^0.5.7",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "postcss": "^7.0.17",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.4",
    "tslint": "^5.18.0",
    "typescript": "~3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.2"
  },
  "resolutions": {
    "@types/react": "^15.0.16"
  },
  "build": {
    "appId": "Pyllisto",
    "mac": {
      "category": "com.electron.pyllisto"
    }
  }
}
